// Local zone file for localhost
$TTL 604800
@   IN  SOA ns.localhost. root.localhost. (
                2024010101  ; Serial
                604800      ; Refresh
                86400       ; Retry
                2419200     ; Expire
                604800 )    ; Minimum
    IN  NS  ns.localhost.
    IN  A   127.0.0.1
ns  IN  A   127.0.0.1
